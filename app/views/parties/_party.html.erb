<%= form_with model: @party, method: method, local: true do |f| %>
  <%= f.hidden_field :movie_id, value: @movie.id %>
  <%= f.label :duration %>
  <%= f.text_field :duration %>
  <%= f.label :friends %>
  <%= f.collection_check_boxes :invites, current_user.friends, :email, :id %>
  <%= f.submit "Create Party"%>
<% end %>
