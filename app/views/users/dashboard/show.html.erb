<h1> Welcome </h1>

<%=  button_to "Discover Movies", discover_index_path, method: :get %>
<section>
  <%= "Friends" %>
  <% current_user.friends.each do |friend| %>
    <%=  friend.email%>
  <% end %>
</section>
<section>
  <%= "Viewing Parties" %>
  <% current_user.parties.each do |party| %>
    <%=  party.movie.title%>
  <% end %>
</section>